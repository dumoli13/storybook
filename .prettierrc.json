{
  "singleQuote": true,
  "trailingComma": "all",
  "endOfLine": "auto",
  "tabWidth": 2,
  "bracketSpacing": true,
  "importOrder": [
    "^react$",
    "<THIRD_PARTY_MODULES>",
    "^@?\\w",
    "^@api/(.*)$",
    "^@components/(.*)$",
    "^@types/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true 
}
